<!DOCTYPE html>
<html ng-app="myApp"  ng-controller="searchController">
<head>
  <title>Comfystudy</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>

<strong>Logged in as <%= current_user.first_name + " " + current_user.last_name if current_user %></strong> | <%= link_to "Log Out", logout_path %>
<% else %>

<%=link_to "Log In", login_path %> | <%= link_to "Sign Up", signup_path %>

<% end %>

<%= yield %>

</body>
</html>
